{
  "name": "olifanton/boc",
  "description": "BOC (Bag of Cells) PHP serialization library",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/olifanton/boc",
  "keywords": [
    "ton",
    "blockchain",
    "the open network",
    "boc",
    "bag of cells",
    "olifanton"
  ],
  "autoload": {
    "psr-4": {
      "Olifanton\\Boc\\": "src/Olifanton/Boc/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Olifanton\\Boc\\Tests\\": "tests/Olifanton/Boc/Tests"
    }
  },
  "authors": [
    {
      "name": "Roman Zaycev",
      "email": "box@romanzaycev.ru",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1",
    "olifanton/utils": "^0.3",
    "myclabs/deep-copy": "^1.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "XDEBUG_MODE=coverage phpunit"
  }
}
